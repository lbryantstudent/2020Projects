    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="194dp"
        android:layout_height="132dp"
        android:layout_marginBottom="136dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
